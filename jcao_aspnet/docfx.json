{
  "build": {
    "content": [
      {
        "files": [
          "**/*.md",
          "toc.yml",
          "api/**/*.yml"
        ],
        "exclude": [
          "**/obj/**",
          "jcao_aspnet/**",
          "apispec/**"
        ],
        "src": null,
        "dest": null
      }
    ],
    "resource": [
      {
        "files": [
          "**/*.png",
          "**/*.jpg",
          "**/*.gif",
          "**/*.bmp",
          "md/images/**",
          "_themes/**"
        ],
        "exclude": [
          "**/obj/**",
          "_themes/DocPacker/**"
        ]
      }
    ],
    "overwrite": "apispec/*.md",
    "externalReference": [],
    "globalMetadata": {
      "breadcrumb_path": "/jcao_aspnet/toc.json"
    },
    "fileMetadata": {
      "ms.prod":
      {
        "api/**.yml": "azure"
      },
      "ms.service":
      {
        "api/**.yml": "active-directory-b2"
      },
      "ms.technology":
      {
        "api/**.yml": "intune"
      },
      "ms.date":
      {
        "api/**.yml": "Jun 27, 2016"
      },
      "author":
      {
        "api/**.yml": "sharriexie"
      },
      "manager":
      {
        "api/**.yml": "jeffgilb"
      }
    },
    "template": [
      "docs.html"
    ],
    "dest": "jcao_aspnet"
  }
}
